{
  "name": "nicolas-joubert/grabit-front-feed-bundle",
  "description": "Atom feed frontend for GrabitBundle",
  "keywords": ["grab", "crawler", "frontend", "atom"],
  "homepage": "https://github.com/nicolas-joubert/grabit-front-feed-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Joubert",
      "homepage": "https://www.nvirenque.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "NicolasJoubert\\GrabitFrontFeedBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NicolasJoubert\\GrabitFrontFeedBundle\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.3",
    "ext-dom": "*",
    "doctrine/dbal": "^3||^4",
    "doctrine/doctrine-bundle": "^2.11",
    "doctrine/orm": "^3.0||^4.0",
    "nicolas-joubert/grabit-bundle": "^1.0",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "7.2.*",
    "symfony/runtime": "7.2.*",
    "symfony/yaml": "7.2.*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "*",
    "phpstan/extension-installer": "*",
    "phpstan/phpstan": "*",
    "phpstan/phpstan-doctrine": "*",
    "phpstan/phpstan-symfony": "*",
    "phpunit/phpunit": "<10.0",
    "rector/rector": "*"
  },
  "suggest": {
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "phpstan/extension-installer": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  }
}
